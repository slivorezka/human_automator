{
  "manifest_version": 3,
  "name": "Human Automator",
  "version": "1.0.0",
  "description": "An unofficial app for automatization human.ua service",
  "action": {
    "default_icon": {
      "128": "icons/128.png"
    }
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/16.png",
    "128": "icons/16.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.human.ua/*"
      ],
      "js": [
        "content-CH0gEOC2.js"
      ],
      "css": [
        "style-CuCdXjcO.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "icons/logo.svg"
      ],
      "matches": [
        "*://*.human.ua/*"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "*://*.human.ua/*"
  ]
}